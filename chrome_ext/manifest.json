{
  "manifest_version": 3,
  "name": "Check Mate",
  "description": "Check credibility of all tweets making claims.",
  "version": "1.0",
  "action": {
    "default_icon": "logo.png",
    "default_title": "Click to show an alert"
  },
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  }
}
